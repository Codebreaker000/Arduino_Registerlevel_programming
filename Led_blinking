void setup()
{
  DDRB |= (1<<DDB5)|(1<<DDB4)|(1<<DDB3)|(1<<DDB2);
}

void loop()
{
  PORTB|=(1<<PB5);
  delay(1000);
  PORTB&=~(1<<PB5);
  delay(1000);
  PORTB|=(1<<PB4);
  delay(1000);
  PORTB&=~(1<<PB4);
  delay(1000);
  PORTB|=(1<<PB3);
  delay(1000);
  PORTB&=~(1<<PB3);
  delay(1000);
  PORTB|=(1<<PB2);
  delay(1000);
  PORTB&=~(1<<PB2);
  delay(1000);
}
